{
	"name": "ssuggestor",
	"version": "0.2.1",
	"description": "react-simple-suggestor",
	"main": "dist/ssuggestor.js",
	"scripts": {
		"test": "jest",
		"test:snapshot": "npm test -- -u",
		"test:coverage": "jest --coverage",
		"coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
		"tdd": "jest --watch",
		"lint": "eslint . --cache --ext .js --ignore-path .eslintignore --ignore-pattern '**/__tests__/*'",
		"stats": "webpack --profile --json > stats.json",
		"dev": "webpack-dev-server",
		"example": "webpack -p",
		"build": "npm run build:main & npm run build:prod",
		"build:main": "webpack --config webpack.config.prod.js",
		"build:prod": "npm run build:main -- -p --output-filename ssuggestor.min.js",
		"prepublish": "npm run build",
		"precommit": "npm run lint",
		"prepush": "npm run test"
	},
	"files": [
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/carloluis/ssuggestor.git"
	},
	"keywords": [
		"suggestor",
		"react",
		"react16",
		"component",
		"bootstrap",
		"search",
		"highlight",
		"select",
		"form",
		"input",
		"ui"
	],
	"author": "carloluis <carloluisr@gmail.com> (https://github.com/carloluis)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/carloluis/ssuggestor/issues"
	},
	"homepage": "https://github.com/carloluis/ssuggestor#readme",
	"devDependencies": {
		"babel-core": "^6.25.0",
		"babel-jest": "^22.2.2",
		"babel-loader": "^7.1.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"clean-webpack-plugin": "^0.1.16",
		"coveralls": "^3.0.0",
		"enzyme": "^3.2.0",
		"enzyme-adapter-react-16": "^1.1.0",
		"eslint": "^4.4.1",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-react": "^7.2.0",
		"html-webpack-plugin": "^2.30.1",
		"husky": "^0.14.3",
		"jest": "^22.2.2",
		"prop-types": "^15.6.0",
		"react": "^16.1.1",
		"react-dom": "^16.1.1",
		"react-test-renderer": "^16.1.1",
		"webpack": "^3.5.4",
		"webpack-dev-server": "^2.7.1"
	},
	"peerDependencies": {
		"prop-types": "^15.0",
		"react": "^16.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"js"
		],
		"setupFiles": [
			"<rootDir>/__setup__/shim.js",
			"<rootDir>/__setup__/enzyme.js"
		]
	}
}
