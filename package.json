{
  "name": "ssuggestor",
  "version": "0.0.28",
  "description": "react-simple-suggestor",
  "main": "dist/ssuggestor.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .  --cache --ext .js --ignore-path .eslintignore --ignore-pattern '**/__tests__/*'",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "dev": "webpack-dev-server",
    "example": "webpack -p",
    "build": "npm run build:main & npm run build:prod",
    "build:main": "webpack --config webpack.config.publish.js",
    "build:prod": "npm run build:main -- -p --output-filename ssuggestor.min.js",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "test"
  ],
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carloluis/ssuggestor.git"
  },
  "keywords": [
    "suggestor",
    "react",
    "react-component",
    "bootstrap",
    "search",
    "highlight",
    "select",
    "form",
    "input",
    "ui"
  ],
  "author": "carloluis <carloluisr@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carloluis/ssuggestor/issues"
  },
  "homepage": "https://github.com/carloluis/ssuggestor#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^18.1.0",
    "pre-commit": "^1.2.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "15.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  }
}
